{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:Get*",
        "s3:List*"
      ],
      "Resource": "arn:aws:s3:::bucketName*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:DeleteObject",
	"s3:PutObject"
      ],
      "Resource": "arn:aws:s3:::bucketName/push-notifications/*"
    },
    {
      "Effect": "Allow",
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::bucketName/adaptive-ema-policies/*"
    }
  ]
}