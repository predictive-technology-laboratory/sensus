{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/Sensus.Android/bin/Debug/SensusAndroid.dll",
            "**/Sensus.iOS/bin/iPhone/Debug/SensusiOS.exe",
	    "**/Sensus.Shared.NuGet/bin/Debug/netstandard2.0/Sensus.Shared.NuGet.dll",
	    "**/ExampleScriptProbeAgent/bin/Debug/ExampleScriptProbeAgent.dll",
            "**/ExampleSensingAgent/bin/Debug/ExampleSensingAgent.dll"
          ],
          "src": "./src"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Sensus Documentation",
      "_appLogoPath": "images/group-of-members-users-icon.png",
      "_appFaviconPath":  "images/favicon.ico",
      "_appFooter": "Copyright Â© 2014-2018 University of Virginia<br>Generated by <strong>DocFX</strong>",
      "_enableSearch": "true"
    },
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}